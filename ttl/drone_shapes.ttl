# required classes for sub-graph ##############################################
####
# UAV
####
landrs:UAVShape
  a sh:NodeShape ;
  sh:targetClass landrs:UAV;
  rdfs:label 'Drone_shape' ;
#   sh:property [
#     sh:path td:title ;
#     sh:datatype xsd:string ;
#     sh:name "Title" ;
#     sh:description "The title of the UAV." ;
#     sh:minCount 1 ;
#     sh:maxCount 1 ;
#     sh:resultSeverity sh:Violation ;
#     sh:order 0 ;
#   ] ;
sh:property [
    sh:name "Drone" ;
    sh:path td:title ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Drone name' ;
    sh:defaultValue  'CRC Scientific Drone #1' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:resultSeverity sh:Violation ;
    sh:order 1 ;
  ] ;
  sh:property [
    sh:path eqp:aircraftSerialNumber ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:name "Aircraft Serial Number" ;
    sh:description "The manufacturer serial number of the aircraft." ;
    sh:defaultValue  'CRC XX1' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:resultSeverity sh:Violation ;
    sh:order 2 ;
  ] ;
  sh:property [
    sh:path eqp:aircraftTypeDesignator ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:name "Aircraft Type Designator" ;
    sh:description "The type designator code for this type of aircraft." ;
    sh:defaultValue  '(e.g., B777)' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:resultSeverity sh:Violation ;
    sh:order 3 ;
  ] ;
  sh:property [
    sh:path schema:Brand ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:name "Brand" ;
    sh:description "A name used by an organization or business person for labeling the UAV." ;
    sh:defaultValue  'CRC UAV' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:resultSeverity sh:Violation ;
    sh:order 4 ;
  ] ;
  sh:property [
    sh:path schema:model ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:name "Model" ;
    sh:description "The model of the UAV." ;
    sh:defaultValue  'CRC 2020.1' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:resultSeverity sh:Violation ;
    sh:order 5 ;
  ] .
