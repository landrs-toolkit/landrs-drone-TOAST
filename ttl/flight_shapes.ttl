@prefix landrs: <http://schema.landrs.org/schema/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn-ext: <http://www.w3.org/ns/ssn/ext/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix r5r: <http://data.europa.eu/r5r#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix geosparql: <http://www.opengis.net/ont/geosparql#> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix qudt_unit: <http://qudt.org/2.1/vocab/unit#> .
@prefix qudt: <http://qudt.org/2.1/schema/qudt#> .

# required classes for sub-graph ##############################################
####
# Place
####
landrs:Flight_PlaceShape
    a sh:NodeShape ;
    sh:targetClass landrs:Place ;
    rdfs:label 'Flight_shape' ;
# mandatory properties
sh:property [
    sh:path schema:name ;
    sh:datatype xsd:string ;
    sh:name "flight_location" ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Flight location' ;
    sh:defaultValue  '$flight_name,Auto-generated from GCS file' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;     
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path schema:description ;
    sh:datatype xsd:string ;
    sh:name "description" ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Flight description' ;
    sh:defaultValue 'zig-zag accross field' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 1 ;
] ;
sh:property [
    sh:path landrs:hasSpatialFootprint ;
    sh:nodeKind sh:IRI ;
    sh:class geosparql:Geometry ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

####
# ObservationCollectionShape
####
landrs:Flight_ObservationCollectionShape
    a sh:NodeShape ;
    sh:targetClass sosa:ObservationCollection ;
    rdfs:label 'Flight_shape' ;
# Mandatory properties
sh:property [
    sh:path rdfs:label ;
    sh:name 'flight_collection' ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Flight collection name' ;
    sh:defaultValue  '$flight_name,flight_collection' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path dct:description ;
    sh:name 'description' ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Flight description' ;
    sh:defaultValue 'zig-zag accross field' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 1 ;
] ;
sh:property [
    sh:path sosa:madeBySensor ;
    sh:nodeKind sh:IRI ;
    sh:class landrs:Sensor ;
    rdfs:label 'graph_boundary' ;
    sh:name 'sensor' ;
    sh:description 'Select Sensor' ;
    sh:defaultValue 'http://127.0.0.1:5000/id/CfJDfMLMTu2ZcfMcGADlYg' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 5 ; 
] ;
sh:property [
    sh:path sosa:observedProperty ;
    sh:nodeKind sh:IRI ;
    sh:class sosa:ObservableProperty ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Select Observable Property' ;
    sh:name 'observableproperty' ;
    sh:defaultValue 'https://www.ebi.ac.uk/chebi/searchId.do?chebiId=CHEBI:16526' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 4 ;
];
sh:property [
    sh:path prov:wasInformedBy ;
    sh:nodeKind sh:IRI ;
    sh:class landrs:Flight ;
    sh:severity sh:Violation ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path sosa:qualifiedAssociation ;
    sh:nodeKind sh:IRI ;
    sh:class prov:Association ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path sosa:usedProcedure ;
    sh:nodeKind sh:IRI ;
    sh:class sosa:Procedure ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;   
] ;
sh:property [
    sh:path sosa:hasFeatureOfInterest ;
    sh:nodeKind sh:IRI ;
    sh:class sosa:FeatureOfInterest ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Select Feature of Interest' ;
    sh:name 'featureofinterest' ;
    sh:defaultValue 'http://dbpedia.org/resource/Atmosphere_of_Earth' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 3 ; 
] .

####
# Flight
####
landrs:Flight_FlightShape
    a sh:NodeShape ;
    sh:targetClass landrs:Flight ;
    rdfs:label 'Flight_shape' ;
sh:property [
    sh:path schema:name ;
    sh:name "flight" ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Flight name' ;
    sh:defaultValue  'Field survey #1' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;        
    sh:severity sh:Violation ;
    sh:order 0 ;
] ;
sh:property [
    sh:path schema:description ;
    sh:name "description" ;     
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Flight description' ;
    sh:defaultValue 'zig-zag accross field' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 1 ;
] ;
sh:property [
    sh:path landrs:isUndertakenBy ;
    sh:nodeKind sh:IRI ;
    sh:class landrs:UAV ;
    rdfs:label 'graph_boundary' ;
    sh:name "this_uav" ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path landrs:occursAtPlace ;
    sh:nodeKind sh:IRI ;
    sh:class landrs:Place ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path rdfs:label ;
    sh:name 'flight_label' ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Flight label' ;
    sh:defaultValue  '$flight_name' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

####
# Procedure
####
landrs:Flight_ProcedureShape
    a sh:NodeShape ;
    sh:targetClass sosa:Procedure ;
    rdfs:label 'Flight_shape' ;
sh:property [
    sh:path rdfs:comment ;
    sh:name "description" ;  
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Flight description' ;
    sh:defaultValue 'zig-zag accross field' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 1 ;
] ;
sh:property [
    sh:path ssn:hasInput ;
    sh:name "mission_file" ;   
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Select mission file' ;
    sh:defaultValue '../ardupilot/Tools/autotest/ArduPlane_Tests/Mission/CMAC-soar.txt' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 2 ;
] ;
sh:property [
    sh:path ssn:hasOutput ;
    sh:nodeKind sh:IRI ;
    sh:class landrs:UAV ;
    rdfs:label 'graph_boundary' ;
    sh:name "this_uav" ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

####
# Association
####
landrs:Flight_AssociationShape
    a sh:NodeShape ;
    sh:targetClass prov:Association ;
    rdfs:label 'Flight_shape' ;
sh:property [
    sh:path prov:hadPlan ;
    sh:nodeKind sh:IRI ;
    sh:class sosa:Procedure ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path prov:hadRole ;
    sh:nodeKind sh:IRI ;
    sh:class prov:Role ;
    rdfs:label 'graph_boundary' ;
    sh:defaultValue 'https://www.wikidata.org/wiki/Q81060355' ;
    sh:name "role" ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path prov:Agent ;
    sh:nodeKind sh:IRI ;
    sh:class prov:Agent ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Select Pilot' ;
    sh:name "agent" ;
    sh:defaultValue 'http://orcid.org/0000-0001-8214-7177/' ; # Chris
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 6 ;
] .

####
# Geometry
####
landrs:Flight_GeometryShape
    a sh:NodeShape ;
    sh:targetClass geosparql:Geometry ;
    rdfs:label 'Flight_shape' ;
sh:property [
    sh:path rdfs:label ;
    sh:name 'mission_file' ;
    sh:datatype xsd:string ;
    rdfs:label 'graph_boundary' ;
    sh:description 'Select mission file' ;
    sh:defaultValue '../ardupilot/Tools/autotest/ArduPlane_Tests/Mission/CMAC-soar.txt' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
    sh:order 2 ;
] ;
sh:property [
    sh:path locn:geometry ;
    sh:nodeKind sh:IRI ;
    sh:class locn:geometry ;
    rdfs:label 'graph_boundary' ;
    sh:name "geometry" ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

# boundary graph constraints ##################################################
####
# Flight ObservableProperty, must be property of FeatureOfInterest
####
landrs:Flight_ObservablePropertyShape
    a sh:NodeShape ;
    sh:targetClass sosa:ObservableProperty ;
    rdfs:label 'Flight_constraint' ;
sh:property [
    sh:path ssn:isPropertyOf ;
    sh:nodeKind sh:IRI ;
    sh:class sosa:FeatureOfInterest ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

####
# Flight Sensor, must have observes ObservableProperty
####
landrs:Flight_SensorShape
    a sh:NodeShape ;
    sh:targetClass landrs:Sensor ;
    rdfs:label 'Flight_constraint' ;
sh:property [
    sh:path sosa:observes ;
    sh:nodeKind sh:IRI ;
    sh:class sosa:ObservableProperty ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

# storage #####################################################################
####
# Storage Observation Collection
####
landrs:Store_ObservationCollectionShape
    a sh:NodeShape ;
    sh:targetClass sosa:ObservationCollection ;
    rdfs:label 'Store_shape' ;
sh:property [
    sh:path prov:startedAtTime ;
    sh:datatype xsd:dateTime ;
    sh:name 'startTime' ;
    rdfs:label 'startTime' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path sosa:hasMember ;
    sh:nodeKind sh:IRI ;
    sh:class sosa:Observation ;
    sh:minCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path sosa:madeBySensor ;
    sh:nodeKind sh:IRI ;
    sh:class landrs:Sensor ;
    rdfs:label 'sensor_type' ;
    sh:name 'sensor_type' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

####
# Storage Observation
####
landrs:Store_ObservationShape
    a sh:NodeShape ;
    sh:targetClass sosa:Observation ;
    rdfs:label 'Store_shape' ;
sh:property [
    sh:path sosa:madeBySensor ;
    sh:nodeKind sh:IRI ;
    sh:class landrs:Sensor ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path sosa:hasResult ;
    sh:nodeKind sh:BlankNode ;
    sh:class qudt:QuantityValue ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path sosa:resultTime ;
    sh:nodeKind sh:BlankNode ;
    sh:class xsd:dateTime ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

####
# Storage dateTime
####
landrs:Store_dateTime
    a sh:NodeShape ;
    sh:targetClass xsd:dateTime ;
    rdfs:label 'Store_shape' ;
sh:property [
    sh:path xsd:dateTimeStamp ;
    sh:datatype xsd:dateTime ;
    sh:name 'startTime' ;
    rdfs:label 'startTime' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

####
# Storage Quantity
####
landrs:Store_Quantity
    a sh:NodeShape ;
    sh:targetClass qudt:QuantityValue ;
    rdfs:label 'Store_shape' ;
sh:property [
    sh:path qudt:numericValue ;
    sh:datatype xsd:double ;
    rdfs:label 'observation_result_quantity' ;
    sh:name 'observation_result_quantity' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path qudt:unit ;
    sh:datatype qudt_unit:PPM ;
    rdfs:label 'observation_result_quantity_unit' ;
    sh:name 'observation_result_quantity_unit' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path geosparql:hasGeometry ;
    sh:nodeKind sh:BlankNode ;
    sh:class geosparql:Geometry ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

####
# Storage Geometry
####
landrs:Store_GeoGeometry
    a sh:NodeShape ;
    sh:targetClass geosparql:Geometry ;
    rdfs:label 'Store_shape' ;
sh:property [
    sh:path geosparql:asWKT ;
    sh:datatype geosparql:wktLiteral ;
    rdfs:label 'observation_result_quantity_geo_fix' ;
    sh:name 'observation_result_quantity_geo_fix' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .

####
# Storage Observation Collection, end
####
landrs:Store_End_ObservationCollectionShape
    a sh:NodeShape ;
    sh:targetClass sosa:ObservationCollection ;
    rdfs:label 'Store_shape_end' ;
sh:property [
    sh:path prov:endedAtTime ;
    sh:datatype xsd:dateTime ;
    sh:name 'endTime' ;
    rdfs:label 'endTime' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .
