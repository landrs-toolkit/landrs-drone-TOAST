# end storage #################################################################
####
# Storage Observation Collection, end
####
landrs:Store_End_ObservationCollectionShape
    a sh:NodeShape ;
    sh:targetClass sosa:ObservationCollection ;
    rdfs:label 'Store_shape_end' ;
    sh:name 'the_observation_collection' ;
sh:property [
    sh:path prov:endedAtTime ;
    sh:datatype xsd:dateTime ;
    sh:name 'endTime' ;
    rdfs:label 'graph_boundary' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .
