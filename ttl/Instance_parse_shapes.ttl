################################################################################
# parse class instance data ###################################################
####
# Flight Sensor, must have observes ObservableProperty
####
landrs:Instance_SensorShape
    a sh:NodeShape ;
    sh:targetClass landrs:Sensor ;
    rdfs:label 'Instance_parse' ;
    sh:name 'sensor_instance' ;
sh:property [
    sh:path qudt:hasUnit ;
    sh:datatype qudt:unit ;
    sh:name 'units' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] ;
sh:property [
    sh:path schema:identifier  ;
    sh:datatype schema:identifier  ;
    sh:name 'id' ;
    sh:minCount 1 ;
    sh:maxCount 1 ;
    sh:severity sh:Violation ;
] .
